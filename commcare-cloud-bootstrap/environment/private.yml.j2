secrets:
  ECRYPTFS_PASSWORD: '{{ generate_password() }}'
  KSPLICE_ACTIVATION_KEY:
  DATADOG_API_KEY: null
  DATADOG_APP_KEY: null
  AMQP_USER: "{{ deploy_env }}_worker"
  AMQP_PASSWORD: '{{ generate_password() }}'
  HAPROXY_STATS_PASSWORD: '{{ generate_password() }}'
  SUPERVISOR_HTTP_USERNAME: 'supervisor_http'
  SUPERVISOR_HTTP_PASSWORD: '{{ generate_password() }}'
  POSTGRES_USERS:
    root:
      username: 'root'
      password: '{{ generate_password() }}'
      privs:
        - objs: commcarehq
          type: group
    commcare:
      username: 'commcarehq'
      password: '{{ generate_password() }}'
    devreadonly:
      username: 'devreadonly'
      password: '{{ generate_password() }}'
      role_attr_flags: 'NOSUPERUSER,NOCREATEROLE,NOCREATEDB'
      privs:
        -
          privs: SELECT
          objs: ALL_IN_SCHEMA
    replication:
        username: 'hqrepl'
        password: '{{ generate_password() }}'
        role_attr_flags: 'LOGIN,REPLICATION'

